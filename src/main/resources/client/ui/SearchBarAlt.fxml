<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox fx:id="searchBar" prefHeight="41.0" prefWidth="560.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Gui.SearchBar.SearchBarPresenter">
   <children>
      <TextField fx:id="searchTextField" prefHeight="30.0" prefWidth="491.0" promptText="Enter a songName name...">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="goButton" mnemonicParsing="false" onAction="#onSubmit" onMouseClicked="#onSubmit" prefHeight="39.0" prefWidth="50.0" text="Go">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</HBox>
